{
  "name": "fhir-converter",
  "version": "0.0.1",
  "description": "OAS (Swagger v3) API Definition for Template API",
  "scripts": {
    "lint": "node_modules/.bin/openapi-generator-cli validate -i specification/fhir-converter.yaml",
    "publish": "./node_modules/.bin/openapi-generator-cli generate -i specification/fhir-converter.yaml -g openapi -o build",
    "serve": "node_modules/.bin/speccy serve build/fhir-converter.json",
    "check-licenses": "node_modules/.bin/license-checker --failOn GPL --failOn LGPL"
  },
  "author": "NHS Digital",
  "license": "MIT",
  "homepage": "https://github.com/NHSDigital/fhir-converter",
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.12",
    "newman-reporter-htmlextra": "^1.20.1"
  },
  "devDependencies": {
    "apigeetool": "^0.14.2",
    "license-checker": "^25.0.1",
    "minimist": "^1.2.2",
    "newman": "^4.5.7",
    "newman-reporter-html": "^1.0.5"
  }
}
